{
  "name": "rollup-config-badger-den",
  "version": "0.0.0",
  "homepage": "https://github.com/trioderegion/badger-den",
  "description": "Configurable rollup plugin/template for bundling FVTT modules.",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./index.mjs"
  },
  "scripts": {
    "build": "rollup --config",
    "release": "np",
    "test-src": "rollup --c src/rollup-config-badger-den.mjs --config-den src/demo-module/src/demo-module:dev",
    "test": "rollup --config dist/index.mjs --config-den dist/demo-module/src/demo-module:dev"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "autoprefixer": "^10.4.14",
    "glob": "^10.2.6",
    "locale-codes": "^1.3.1",
    "postcss": "^8.4.24",
    "postcss-preset-env": "^8.4.1",
    "postcss-scss": "^4.0.6",
    "rollup-merge-config": "^0.0.12",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-copy-watch": "^0.0.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "sass": "^1.62.1"
  },
  "devDependencies": {
    "@shon/tiny": "^2.3.0",
    "@yelo/rollup-node-external": "^1.0.1",
    "np": "^7.7.0",
    "rollup-plugin-node-externals": "^6.0.1"
  },
  "keywords": [
    "foundry vtt",
    "fvtt",
    "module",
    "rollup",
    "bundle",
    "bundling"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/trioderegion/badger-den.git"
  },
  "bugs": {
    "url": "https://github.com/trioderegion/badger-den/issues"
  },
  "overrides": {
    "@rollup/pluginutils": "^5.0.2",
    "micromatch": "^4.0.0"
  },
  "author": "BadgerWerks (https://www.ko-fi.com/badgerwerks)",
  "contributors": [
    "trioderegion (https://www.github.com/trioderegion)"
  ],
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/badgerwerks"
  }
}
