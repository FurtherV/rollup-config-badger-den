{
  "version": "1.0.0",
  "title": "Badger Den Demo Module",
  "description": "Demonstration module built from the Badger Den",
  "projectUrl": "https://www.npmjs.com/",
  "entryPoints": {
    "main": "index.mjs",
    "style": "scss/*",
    "external": "lib/index.js",
    "lang": "static/lang/**"
  },
  "build": {
    "dev":{
