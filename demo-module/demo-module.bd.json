    "release":{
      "dest":"./build",
      "compress":true,
      "sourcemaps": true,
      "clean": true
    }
  },
  "dependencies": {
    "core": [
      "10",
      "11.299",
      "12"
    ]
  },
  "static": [
    "static"
  ],
  "authors": [
    {
      "name": "trioderegion",
      "url": "https://www.github.com/trioderegion"
    }
  ]
