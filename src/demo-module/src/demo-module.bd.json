{
  "version": "1.0.0",
  "title": "Badger Den Demo Module",
  "description": "Demonstration module built from the Badger Den",
  "projectUrl": "https://www.npmjs.com/",
  "entryPoints": {
    "main": "index.mjs",
    "style": "scss/*",
    "external": "lib/tiny.mjs",
    "lang": "static/lang/**",
    "compendia": {
      "path": "packs/*/",
      "label": {
        "demo-items": "Demo Items"
      },
      "banner": {
        "sgeh-items": "static/assets/banner.webp"
      }

    }
  },
  "profile": {
    "dev":{
      "dest":"../",
      "compress":false,
      "sourcemaps": true,
      "clean": false,
      "hmrStatic": true,
      "entryPoints": {
        "main": "dev/buildHTML.mjs"
      },
      "profileStatic": ["dev/html"]
    },
    "release":{
      "dest":"../",
      "compress":true,
      "sourcemaps": true,
      "clean": true
    }
  },
  "dependencies": {
    "core": [
      "10",
      "11.299",
      "12"
    ]
  },
  "static": [
    "static"
  ],
  "authors": [
    {
      "name": "trioderegion",
      "url": "https://www.github.com/trioderegion"
    }
  ]
}
